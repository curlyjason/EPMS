{
    "name": "your-name-here/Stacks",
    "description": "Stacks plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "pakacuda/cakephp-fixture-factories": "^0.2.28"
    },
    "autoload": {
        "psr-4": {
            "Stacks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stacks\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
